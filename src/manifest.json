{
  "manifest_version": 2,
  "name": "Fallen London Message Notifications",
  "description": "This extension for Fallen London notifies you when new messages arrive.",
  "permissions": [
    "storage"
  ],
  "version": VERSION,
  "content_scripts": [
    {
      "matches": ["http://fallenlondon.storynexus.com/Gap/Load*"],
      "css": [
        "styles.css"
      ],
      "js": [
        "content-script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
